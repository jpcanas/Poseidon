
<section class="space-y-4">
    <h2 class="text-2xl font-bold">User Setting</h2>
    <div class="card p-4 bg-white shadow-xl">
        <div class="flex-col px-3 pb-3">
            <div class="my-4 flex justify-end">
                <button class="btn btn-primary btn-outline" onclick="modalAddUser.showModal()">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M12 4.5v15m7.5-7.5h-15" />
                    </svg>
                    Add User
                </button>
            </div>
            <div id="myGrid" class="" style="height: 600px; width:100%;"></div>
        </div>
    </div>

    @await Html.PartialAsync("~/Views/Setting/UserSetting_Components/_RegisterUserModal.cshtml")
</section>


@section Scripts {
    <script src="~/js/app/Setting/usersetting.js" asp-append-version="true"></script>
}